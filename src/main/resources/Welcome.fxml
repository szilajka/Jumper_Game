<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="jumper.controllers.WelcomeController">
    <children>
        <Button fx:id="btnLogin" layoutX="250.0" layoutY="60.0" mnemonicParsing="false" onKeyPressed="#btnLoginPressed"
                onMouseClicked="#btnLoginClicked" prefHeight="100.0" prefWidth="300.0" text="Login"/>
        <Button fx:id="btnRegister" layoutX="250.0" layoutY="250.0" mnemonicParsing="false"
                onKeyPressed="#btnRegisterPressed" onMouseClicked="#btnRegisterClicked" prefHeight="100.0"
                prefWidth="300.0" text="Register"/>
        <Button fx:id="btnExit" layoutX="250.0" layoutY="424.0" mnemonicParsing="false" onKeyPressed="#btnExitPressed"
                onMouseClicked="#btnExitClicked" prefHeight="100.0" prefWidth="300.0" text="Exit"/>
    </children>
</AnchorPane>
