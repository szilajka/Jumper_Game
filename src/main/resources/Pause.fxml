<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pauseAP" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Button fx:id="btnContinue" layoutX="236.0" layoutY="104.0" mnemonicParsing="false"
                onKeyPressed="#OnBtnContinuePressed" onMouseClicked="#OnBtnContinueClicked" prefHeight="83.0"
                prefWidth="328.0" text="Continue"/>
        <Button fx:id="btnExit" layoutX="236.0" layoutY="433.0" mnemonicParsing="false" onKeyPressed="#OnBtnExitPressed"
                onMouseClicked="#OnBtnExitClicked" prefHeight="83.0" prefWidth="328.0" text="Exit"/>
        <Button fx:id="btnMenu" layoutX="236.0" layoutY="259.0" mnemonicParsing="false" onKeyPressed="#OnBtnMenuPressed"
                onMouseClicked="#OnBtnMenuClicked" prefHeight="83.0" prefWidth="328.0" text="Menu"/>
        <Label fx:id="lblTime" layoutX="39.0" layoutY="30.0" text="Time in game:">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Label fx:id="lblTimeQuery" layoutX="236.0" layoutY="21.0" prefHeight="45.0" prefWidth="533.0">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
